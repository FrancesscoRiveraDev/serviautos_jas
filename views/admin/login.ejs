    height: auto; /* Mantiene la relación de aspecto */
}


.img2 {
    max-width: 70%;
    height: auto; /* Mantiene la relación de aspecto */
}

/* Estilos específicos para el logo */
.logo {
    max-width: 200px; /* Ajusta este valor según tus necesidades */
    max-height: 200px; /* Ajusta este valor según tus necesidades */
